CREATE TABLE contentList(
     _id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
     name TEXT NOT NULL,
     last_modified_at INTEGER NOT NULL DEFAULT 0
);

insert:
INSERT OR IGNORE INTO contentList(_id, name, last_modified_at)
VALUES (NULL,  :name, strftime('%s', 'now'));

update:
UPDATE contentList SET
    name = COALESCE(:name, name)
WHERE _id = :id;

listCount:
SELECT COUNT(_id) FROM contentList;

delete:
DELETE FROM contentList WHERE _id = :id;

lastInsertRowId:
SELECT last_insert_rowid();